function [ClassColors] = IL_classColors(classes,comparison)

to_comp = find(cellfun(@iscell,comparison));
ClassColors = zeros(size(classes,1),3);
if isempty(to_comp) % just 1 dimensional
    class_types = {'W', 'P', 'S', 'WP', 'WS', 'WPS'};
    [cmap] = master_ColorMaps('plasma',6);
    for c = 1:length(classes)
        ClassColors(c,:) = cmap(strcmp(classes{c},class_types),:);
    end
    
elseif length(comparison{to_comp})==2
    
    
    cmap = [master_ColorMaps('winter',6);...
    	    master_ColorMaps('spring',6);...
            master_ColorMaps('PuOr',6)];
    
    for ll = 1:length(comparison{to_comp})
        
        base = repmat(comparison{to_comp}(ll),;
        if to_comp==1
            base = [comparison{to_comp}{ll} '_' comparison{2} '_' comparison{3}];
        elseif to_comp==2
            base = [comparison{1} '_' comparison{to_comp}{ll} '_' comparison{3}];
        elseif to_comp==3
            base = [comparison{1} '_' comparison{2} '_' comparison{to_comp}{ll}];
        end
        
        for c = 1:size(classes,1)
            
            
            
            
            
            
            
        end
    end
    
elseif length(comparison{to_comp})==3
    error('Not done yet')
    
end


end